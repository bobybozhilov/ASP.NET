<!--To set IntelliSense to the concrete model: write <at>model <Concrete Model> -->
@model IEnumerable<OdeToFood.Models.Restaurant>

<!--write "<html" and double tap "Tab" to auto-create html code-->
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>OdeToFood</title>
</head>
<body>
    <h1>Welcome!</h1>

    <table>
        @foreach (var restaurant in Model)
        {
            <tr>
                <td>@restaurant.Id</td>
                <td>@restaurant.Name</td>
            </tr>
        }
    </table>

</body>
</html>